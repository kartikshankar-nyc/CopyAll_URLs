{
    "manifest_version": 3,
    "name": "Tab URL Copier",
    "version": "1.0",
    "description": "Copy all URLs from open tabs in the current window",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": [
      "tabs",
      "clipboardWrite"
    ]
  }